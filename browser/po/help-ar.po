msgstr ""
"Ctrl</kbd><span class=\"kbd--plus\">+</span><kbd>Alt</kbd><span class=\"kbd--"
"plus\">+</span><kbd>C"

#: html/lool-help.html%2Bdiv.div.div.table.tr.td:701-6
msgid "Insert soft hyphen"
msgstr "إدراج شَرطة اختيارية"

